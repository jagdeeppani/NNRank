set(plot1(3),'LineStyle','--','Color',cl(3,:),'DisplayName','XRAY');
set(plot1(4),'Marker','x','Color',cl(4,:),'DisplayName','ER-SPA');
set(plot1(5),'Marker','square','Color',cl(5,:),'DisplayName','ER-XRAY');

% set(plot1(6),'Marker','+','DisplayName','LP-rho1','Color',[0 0 0]);
