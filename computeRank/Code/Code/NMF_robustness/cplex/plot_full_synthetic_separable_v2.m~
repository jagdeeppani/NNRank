set(plot1(7),'Marker','<','Color',cl(7,:),'DisplayName','true-K');

% Create xlabel
xlabel('Noise level ($\delta$)','Interpreter','latex','FontWeight','bold',...
    'FontSize',40);

% Create ylabel
